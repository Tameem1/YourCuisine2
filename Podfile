  pod ‘GoogleMaps’
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  # Pods for Your Cuisine

  target 'Your CuisineTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Your CuisineUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
